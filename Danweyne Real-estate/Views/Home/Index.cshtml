<div class="content-area home-area-1 recent-property" style="background-color: #FCFCFC; padding-bottom: 55px;">
    <div class="container">

        <div class="row">
            <div class="col-md-10 col-md-offset-1 col-sm-12 text-center page-title">
                <!-- /.feature title -->
                <h2>Top submitted property</h2>
                <p>Searching for your ideal home or commercial property in the UAE shouldn't be a daunting experience. </p>
            </div>
        </div>
            <div class="proerty-th">
                @foreach (var item in Model.Properties)
                {
                var str = item.PropertyName.Replace(" ", "-");
                    <div class="col-sm-6 col-md-3 p0">
                        <div class="box-two proerty-item">
                            <div class="item-thumb">
                            <a href="/property/@item.PropertyId-@str"><img src="@item.CoverImageUrl"></a>
                        </div>
                            <div class="item-entry overflow">
                            <h5 class="summary-text"><a href="/property/@item.PropertyId-@str">@item.PropertyName </a></h5>
                                <div class="dot-hr"></div>
                                <span class="pull-left"><b>Area :</b> @item.Area  sq.m </span>
                            <span class="proerty-price pull-right">$@item.PropertyPrice.ToString("#,##0")</span>
                            </div>
                        </div>
                    </div>
                }


                <div class="col-sm-6 col-md-3 p0">
                    <div class="box-tree more-proerty text-center">
                        <div class="item-tree-icon">
                            <i class="fa fa-th"></i>
                        </div>
                        <div class="more-entry overflow">
                        <h5><a href="/properties">CAN'T DECIDE ? </a></h5>
                            <h5 class="tree-sub-ttl">Show all properties</h5>
                        <button class="btn border-btn more-black" value="All properties" onclick=" window.location.replace('/properties')">All properties</button>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>

